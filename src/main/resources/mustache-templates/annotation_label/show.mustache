{{>layout/header}}
<div class="container">
    <h1>{{entityName}}</h1>
    {{>components/messages}}
    <div>
        {{#entity}}
        <h5>Annotation schema</h5>
        <p>
            {{#annotationSchema}}{{annotationSchema.label}}{{/annotationSchema}}
            {{^annotationSchema}}<span class="text-muted">None</span>{{/annotationSchema}}
        </p>
        <h5>Display order</h5>
        <p>
            {{#displayOrder}}{{.}}{{/displayOrder}}
            {{^displayOrder}}<span class="text-muted">None</span>{{/displayOrder}}
        </p>
        <h5>Accent color</h5>
        <p>
            {{#accentColor}}{{.}}{{/accentColor}}
            {{^accentColor}}<span class="text-muted">None</span>{{/accentColor}}
        </p>
        <h5>Display label</h5>
        <p>
            {{#displayLabel}}{{.}}{{/displayLabel}}
            {{^displayLabel}}<span class="text-muted">None</span>{{/displayLabel}}
        </p>
        <h5>Output label</h5>
        <p>
            {{#outputLabel}}{{.}}{{/outputLabel}}
            {{^outputLabel}}<span class="text-muted">None</span>{{/outputLabel}}
        </p>
        {{>components/show_links}}
        {{/entity}}
    </div>
</div>
{{>layout/footer}}