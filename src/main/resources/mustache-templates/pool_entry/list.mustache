{{>layout/header}}
<div class="container">
    <h1>{{entityName}} List</h1>
    {{>components/messages}}
    <a href="{{req.contextPath}}{{baseRoute}}/new" class="new-entity btn btn-success"><i class="fas fa-plus-circle"></i> New</a>
    <table class="table table-striped table-bordered sorted">
        <thead>
        <tr>
            <th>Pool</th>
            <th>Topic</th>
            <th>Sort order</th>
            <th>Document id</th>
            <th>Frequency</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {{#entity_list}}
        <tr>
            <td>{{#pool}}{{pool.label}}{{/pool}}</td>
            <td>{{#topic}}{{topic.label}}{{/topic}}</td>
            <td>{{#sortOrder}}{{.}}{{/sortOrder}}</td>
            <td>{{#documentId}}{{.}}{{/documentId}}</td>
            <td>{{#frequency}}{{.}}{{/frequency}}</td>
            <td>{{>components/list_actions}}</td>
        </tr>
        {{/entity_list}}
        </tbody>
    </table>
</div>
{{>layout/footer}}
